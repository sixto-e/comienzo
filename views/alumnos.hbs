<h1>alumnos:</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Matricula</th>
      <th scope="col">Nombre</th>
      <th scope="col">Localidad</th>
      <th scope="col">Numero de carrera</th>
      <th scope="col">ver mas</th>
    </tr>
  </thead>
 
  <tbody>
       {{#each alumnos}}
    <tr>
      <td>{{matricula}}</td>
      <td>{{nombre}}</td>
      <td>{{localidad}}</td>
      <td>{{no_carrera}}</td>
      <td><a href="/alumnos/single/{{matricula}}">ver mas</a></td>
    </tr>
      {{/each}}
  </tbody>

</table>